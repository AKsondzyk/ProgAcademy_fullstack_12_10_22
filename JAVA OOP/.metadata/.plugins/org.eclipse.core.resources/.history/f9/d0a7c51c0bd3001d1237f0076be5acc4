package javaooplectionfive;

import java.io.File;
import java.io.IOException;

public class Main {

	public static void main(String[] args) {

		File in = new File("/Prog Academy/DZ/Examples/10/magazine-main/diy-woman.png");
	    
	    File out = new File(in.getName());
	    
	    try {
	      System.out.println(FileService.copyFile(in, out));
	    } catch (IOException e) {
	      // TODO Auto-generated catch block
	      e.printStackTrace();
	    }
	}
}
